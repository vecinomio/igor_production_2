
    <Connector port="<%= @ssl_port %>" SSLEnabled="true" scheme="https" secure="true"<% -%>
           <%- if @ssl_params_real and ! @ssl_params_real.empty? -%>
             <%- @ssl_params_real.each_pair do |attrib, value| %>
               <%= attrib %>=<%= "#{value}".encode(:xml => :attr) -%>
             <%- end -%>
           <%- end %> />
